{
    "name": "iee",
    "description": "iee",
    "version": "1.1.0",
    "keywords": [],
    "scripts": {
        "start": "forever --minUptime 1000 --spinSleepTime 1000 app.js",
        "start-dev": "nodemon -e js,json,html app.js",
        "lint": "eslint --ignore-pattern apps/api/apidoc ."
    },
    "dependencies": {
        "koa": "1.2.x",
        "koa-router": "5.4.x",
        "koa-views": "4.1.x",

        "co": "4.6.x",
        "mysql-co": "^0.0.4",
        "koa-body": "^1.4.0",
        "koa-json": "^1.1.3",
        "koa-session": "^3.3.1",

		"ali-oss": "~4.4.4",
		"uuid": "~2.0.2",
		"moment": "~2.13.0",

        "md5": "~2.1.0"
    },
    "devDependencies": {
        "gulp": "3.x.x",
        "gulp-cssnano": "2.x.x",
        "gulp-less": "3.x.x",
        "gulp-uglify": "1.x.x",
        "webpack": "^1.12.14",
        "nunjucks": "2.4.x",

        "chai": "^3.5.0",
        "cheerio": "^0.20.0",
        "co-mocha": "^1.1.2",
        "co-supertest": "^0.0.10",
        "eslint": "^2.11.1",
        "mocha": "^2.5.3",
        "nodemon": "^1.9.2",
        "npm-check": "^5.2.1",
        "npm-check-updates": "^2.6.5",
        "supertest": "^1.2.0",

        "atool-build": "0.7.x",
        "babel-plugin-antd": "0.4.x",
        "babel-loader": "^6.2.4",
        "babel-core": "^6.9.1",
        "babel-preset-es2015": "^6.9.0",
        "babel-plugin-transform-runtime": "^6.8.0",
        "babel-runtime": "^6.6.1"
    },
    "eslintConfig": {
        "env": {
            "node": true,
            "es6": true,
            "mocha": true
        },
        "extends": "eslint:recommended",
        "rules": {
            "comma-dangle": ["error", "always-multiline"],
            "curly": ["error", "multi-line"],
            "indent": ["error", 4, {
                "SwitchCase": 1
            }],
            "key-spacing": ["error", {
                "align": "value"
            }],
            "no-console": "warn",
            "no-var": "error",
            "object-curly-spacing": ["error", "always"],
            "prefer-const": "error",
            "quotes": ["error", "single", "avoid-escape"],
            "semi": ["error", "always"],
            "strict": ["error", "global"]
        }
    },
    "engines": {
        "node": "^6.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/moldauder/iee.git"
    },
    "author": "moldauder",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/moldauder/iee/issues"
    },
    "homepage": "https://github.com/moldauder/iee#readme"
}
